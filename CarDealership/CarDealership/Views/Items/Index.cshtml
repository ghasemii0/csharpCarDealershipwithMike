@using cardealership;
<h1>Here is a list of your cars</h1>
<ul>
  <li> Length: @Model.Count</li>
  @if (Model.Count ==0)
  {
    <p>There are no cars to show</p>
  }
  @if(Model.Count !=0)
  {
    @foreach(Car car in Model)
    {
      <li>Model: <a href='/Show/@car.getId()'>@car.getModel()</a></li>
    }

  }
</ul>
<a class="nav-link" href="/Index/new">Add New Car</a>
<form action="/Index/delete" method="post">
  <button type="submit" name="button">Clear All Items</button>
</form>
